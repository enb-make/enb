{
  "name": "enb",
  "version": "0.17.2",
  "description": "Faster BEM/BEViS assembler",
  "keywords": [
    "enb",
    "bem",
    "bevis",
    "assembler",
    "make",
    "server"
  ],
  "author": "Marat Dulin <mdevils@yandex.ru>",
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/enb-make/enb/blob/master/LICENSE"
    }
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:enb-make/enb.git"
  },
  "homepage": "https://github.com/enb-make/enb",
  "bugs": "https://github.com/enb-make/enb/issues",
  "maintainers": [
    {
      "name": "Andrew Abramov",
      "email": "andrewblond@yandex.com",
      "github-username": "blond"
    },
    {
      "name": "Marat Dulin",
      "email": "mdevils@yandex.ru",
      "github-username": "mdevils"
    }
  ],
  "contributors": [
    "Andrew Abramov <andrewblond@yandex.com>",
    "Marat Dulin <mdevils@yandex.ru>"
  ],
  "engines": {
    "node": ">= 0.10.0"
  },
  "dependencies": {
    "borschik": "1.0.0",
    "clear-require": "1.0.1",
    "commander": "1.1.1",
    "connect": "3.1.1",
    "dom-js": "0.0.9",
    "enb-source-map": "1.7.2",
    "inherit": "~2.1.0",
    "js-yaml": "2.1.0",
    "lodash": "3.10.1",
    "mad": ">=0.4.0",
    "madify": ">=0.0.1",
    "mime": "1.2.11",
    "puml-link": "0.0.1",
    "send": "0.8.3",
    "serve-static": "1.5.3",
    "sibling": ">=0.0.3",
    "stack-trace": "0.0.9",
    "stylus": "0.40.x",
    "vow": "~0.3.9",
    "vow-fs": "~0.2.2",
    "worker-farm": "1.3.1"
  },
  "devDependencies": {
    "bh": "0.1.14",
    "chai": "3.2.0",
    "chai-as-promised": "5.1.0",
    "coveralls": "~2.11.2",
    "deasync": "0.1.0",
    "enb-validate-code": "0.0.1",
    "istanbul": "0.2.10",
    "jscs": "~1.2.3",
    "jshint": "2.1.3",
    "mocha": "2.1.0",
    "mock-enb": "0.0.2",
    "mock-fs": "2.7.1",
    "sinon": "1.7.3",
    "sinon-chai": "2.8.0"
  },
  "bin": {
    "enb": "./bin/enb"
  },
  "main": "lib/api/index",
  "scripts": {
    "test": "npm run jshint && npm run check-style && npm run func-test && npm run unit-test",
    "jshint": "jshint .",
    "unit-test": "mocha --recursive test/lib",
    "func-test": "npm run build-sample-proj && mocha --recursive test/func",
    "build-sample-proj": "node ./bin/enb make -n --graph --dir test/fixtures/sample-project",
    "check-style": "jscs -c node_modules/enb-validate-code/jscs.json lib exlib techs test",
    "coverage": "istanbul cover _mocha -- -R dot --recursive test/lib",
    "coveralls": "npm i coveralls && istanbul cover _mocha -- -R dot --recursive test/lib --report lcovonly && cat ./coverage/lcov.info | coveralls"
  }
}
